    "handlebars": "*",
    "metalsmith": "*",
    "metalsmith-assets": "*",
    "metalsmith-collections": "*",
    "metalsmith-layouts": "*",
    "metalsmith-markdown": "*",
    "metalsmith-permalinks": "*",
    "nodemon": "^1.11.0"
  }
}
